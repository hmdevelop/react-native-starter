{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "build",
    "allowJs": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "jsx": "react-native",
    "lib": ["dom", "es2015", "es2016", "es2017"],
    "skipLibCheck": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strict": true,
    "target": "es2017",
    "paths": {
      "@apis/*": ["./src/apis/*"],
      "@pages/*": ["./src/components/pages/*"],
      "@navigations/*": ["./src/components/navigations/*"],
      "@models/*": ["./src/models/*"],
      "@actions": ["./src/actions/index"],
      "@stores/*": ["./src/stores/index"],
      "@reducers": ["./src/reducers/index"],
      "@utils/*": ["./src/utils/*"],
      "@STRINGS": ["./STRINGS"],
      "@BaseStyles": ["./src/BaseStyles"],
      "@assets/*": ["./assets/*"],
      "@constants/*": ["./src/constants/*"],
      "@shared/*": ["./src/components/shared/*"],
      "@App_dev": ["./App_dev"],
      "@app": ["./src/app"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "filesGlob": ["typings/index.d.ts", "src/**/*.ts", "src/**/*.tsx"],
  "types": ["react", "react-native", "jest", "typePatches"],
  "exclude": ["android", "ios", "build", "node_modules", "**/*.spec.ts"]
}
